{
  "name": "virtual-list",
  "version": "1.0.0",
  "description": "An a11y friendly web component for virtualized lists, built on top of TanStack Virtual",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "test": "npm run build"
  },
  "keywords": [
    "web-components",
    "virtualization",
    "tanstack"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@tanstack/virtual-core": "^3.13.12"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
